<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Duration_Limit</fullName>
    <active>true</active>
    <errorConditionFormula>Duration_Hours__c &gt; 24</errorConditionFormula>
    <errorDisplayField>Duration_Hours__c</errorDisplayField>
    <errorMessage>Study duration cannot exceed 24 hours.</errorMessage>
</ValidationRule>
